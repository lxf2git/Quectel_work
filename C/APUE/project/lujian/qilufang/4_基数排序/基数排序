 typedef struct node
{
	int data;
	struct node *prev;
	struct node *next;
}sNode,*pNode;

typedef struct dis_chain
{
	pNode record;

	struct dis_chain*next;
}sDisChain,*pDisChain;


num = judge_num();
for(i<num)
{
	distribute();
	collected();	
}
